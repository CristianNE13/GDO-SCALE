<Window
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="ConfiguracionWindow1" x:Class="Scale_Program.ConfiguracionWindow"
    mc:Ignorable="d"
    Title="Missing Parts - Configuracion" Height="756.605" Width="671.566" FontSize="16" FontFamily="Arial"
    Visibility="Visible" Icon="Content/img/menu.png" WindowStartupLocation="CenterScreen" ResizeMode="NoResize"
    WindowStyle="ToolWindow" Loaded="ConfiguracionWindow1_Loaded" Closed="ConfiguracionWindow1_Closed">
    <Window.Effect>
        <DropShadowEffect Color="#FF888888" Opacity="0.5" />
    </Window.Effect>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="781*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="70" />
            <RowDefinition Height="100*" />
            <RowDefinition Height="84" />
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Background="#FF005288"
              Grid.ColumnSpan="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="279*" />
            </Grid.ColumnDefinitions>
            <Label Content="Configuración" HorizontalAlignment="Left" Margin="10,8,0,0" VerticalAlignment="Top"
                   Height="54" FontFamily="Open Sans" FontSize="33.333" Width="268" FontWeight="Bold"
                   Foreground="White" />
        </Grid>
        <TabControl Margin="20,20,20,5" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Grid.Row="1"
                    Grid.ColumnSpan="2">
            <TabItem Header="Bascula-1">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="155*" />
                        <ColumnDefinition Width="461*" />
                    </Grid.ColumnDefinitions>
                    <!--  -->
                    <Label x:Name="lblPuerto_1" Content="Puerto:" HorizontalAlignment="Left" Margin="15,49,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="140"
                           Grid.ColumnSpan="2" />
                    <ComboBox x:Name="cboxScalePort_1"
                              HorizontalAlignment="Left" Margin="125,48,0,0" VerticalAlignment="Top" Width="229"
                              Height="32" Background="White" SelectedIndex="1" FontSize="18" Grid.ColumnSpan="2"
                              FontFamily="Arial" />

                    <!--  -->
                    <Label x:Name="lblFormato_1" Content="Unidades:" HorizontalAlignment="Left" Margin="15,101,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="140"
                           Grid.ColumnSpan="2" />
                    <ComboBox x:Name="cboxUnidades" HorizontalAlignment="Left" Margin="125,101,0,0"
                              VerticalAlignment="Top" Width="229" Height="32" Background="White" FontSize="18"
                              Grid.ColumnSpan="2" SelectedIndex="0" FontFamily="Arial"
                              SelectionChanged="cboxUnidades_SelectionChanged">
                        <ComboBoxItem Content="lb" />
                        <ComboBoxItem Content="kg" />
                    </ComboBox>

                    <Button x:Name="btnZero_1" Content="ZERO" HorizontalAlignment="Left" Margin="324.345,194,0,0"
                            VerticalAlignment="Top" Width="127" Height="48"
                            Background="#FFF9F9F9" Grid.Column="1" Click="btnZero_1_Click" FontSize="18" />

                    <!-- Scale comm log -->
                    <Label x:Name="lblLectura_1" Content="Lectura:" HorizontalAlignment="Left" Margin="15,162,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="175"
                           Grid.ColumnSpan="2" />
                    <ListBox Name="listLogEntries_1" HorizontalAlignment="Left" Height="310" Margin="18,194,0,0"
                             VerticalAlignment="Top" Width="451" FontFamily="Courier New" FontSize="12"
                             Grid.ColumnSpan="2" />
                    <Button x:Name="btnConectar" Content="Conectar" HorizontalAlignment="Left" Margin="325,456,0,0"
                            VerticalAlignment="Top" Width="127" Height="48"
                            Background="#FFF9F9F9" Grid.Column="1" Click="btnConectar_Click" FontSize="18" />
                </Grid>
            </TabItem>
            <TabItem Header="Bascula-2">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="155*" />
                        <ColumnDefinition Width="461*" />
                    </Grid.ColumnDefinitions>
                    <!--  -->
                    <Label x:Name="lblPuerto_2" Content="Puerto:" HorizontalAlignment="Left" Margin="15,49,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="140"
                           Grid.ColumnSpan="2" />
                    <ComboBox x:Name="cboxScalePort_2"
                              HorizontalAlignment="Left" Margin="125,48,0,0" VerticalAlignment="Top" Width="229"
                              Height="32" Background="White" SelectedIndex="1" FontSize="18" Grid.ColumnSpan="2"
                              FontFamily="Arial" />

                    <!--  -->
                    <Label x:Name="lblFormato_2" Content="Unidades:" HorizontalAlignment="Left" Margin="15,101,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="140"
                           Grid.ColumnSpan="2" />
                    <ComboBox x:Name="cboxUnidades2" HorizontalAlignment="Left" Margin="125,101,0,0"
                              VerticalAlignment="Top" Width="229" Height="32" Background="White" FontSize="18"
                              Grid.ColumnSpan="2" SelectedIndex="0" FontFamily="Arial"
                              SelectionChanged="cboxUnidades2_SelectionChanged">
                        <ComboBoxItem Content="lb" />
                        <ComboBoxItem Content="kg" />
                    </ComboBox>

                    <Button x:Name="btnZero_2" Content="ZERO" HorizontalAlignment="Left" Margin="324.345,194,0,0"
                            VerticalAlignment="Top" Width="127" Height="48"
                            Background="#FFF9F9F9" Grid.Column="1" Click="btnZero_2_Click" FontFamily="Arial"
                            FontSize="18" />

                    <Button x:Name="btnConectar2" Content="Conectar" HorizontalAlignment="Left" Margin="325,456,0,0"
                            VerticalAlignment="Top" Width="127" Height="48"
                            Background="#FFF9F9F9" Grid.Column="1" Click="btnConectar_Click" FontSize="18" />

                    <!-- Scale comm log -->
                    <Label x:Name="lblLectura_2" Content="Lectura:" HorizontalAlignment="Left" Margin="15,162,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="175"
                           Grid.ColumnSpan="2" />
                    <ListBox Name="listLogEntries_2" HorizontalAlignment="Left" Height="310" Margin="18,194,0,0"
                             VerticalAlignment="Top" Width="451" FontFamily="Courier New" FontSize="12"
                             Grid.ColumnSpan="2" />
                </Grid>
            </TabItem>
            <TabItem Header="SeaLevel - Zebra">
                <Grid Background="#FFE5E5E5">
                    <Label x:Name="lblPuertoES" Content="Puerto Sealevel:" HorizontalAlignment="Left"
                           Margin="30,24,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="151" />
                    <ComboBox x:Name="cboxPortSeaLevel" HorizontalAlignment="Left" Margin="181,24,0,0"
                              VerticalAlignment="Top" Width="113" Height="32" Background="White" SelectedIndex="1"
                              FontSize="18" FontFamily="Arial" />
                    <Label Content="Dispensadora unitaria" HorizontalAlignment="Left" Margin="25,133,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="196" />
                    <ComboBox x:Name="cboxOutputUnitaria" HorizontalAlignment="Left" Margin="238,133,0,0"
                              VerticalAlignment="Top" Width="98" Height="32" Background="White" FontSize="18"
                              FontFamily="Arial" />
                    <Label Content="Sensor D. unitaria" HorizontalAlignment="Left" Margin="25,268,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="167" />
                    <ComboBox x:Name="cboxInputUnitaria" HorizontalAlignment="Left" Margin="195,268,0,0"
                              VerticalAlignment="Top" Width="99" Height="32" Background="White" FontSize="18"
                              FontFamily="Arial" />
                    <Label Content="Sensor D. prop65" HorizontalAlignment="Left" Margin="25,309,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="167"
                           Visibility="Visible" />
                    <ComboBox x:Name="cboxInputProp65" HorizontalAlignment="Left" Margin="195,309,0,0"
                              VerticalAlignment="Top" Width="99" Height="32" Background="White" FontSize="18"
                              FontFamily="Arial" />
                    <Label x:Name="lblInputs" Content="Asignación de Entradas" Margin="0,222,30,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="21.333" Height="35"
                           Foreground="#FF005288" FontWeight="Bold" BorderBrush="Black" BorderThickness="0,0,0,1"
                           HorizontalAlignment="Right" Width="570" />
                    <Label x:Name="lblOutputs" Content="Asignación de Salidas" Margin="0,87,30,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="21.333" Height="35"
                           Foreground="#FF005288" FontWeight="Bold" BorderBrush="Black" BorderThickness="0,0,0,1"
                           HorizontalAlignment="Right" Width="570" />
                    <Label Content="Dispensadora prop65" HorizontalAlignment="Left" Margin="25,177,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="196" />
                    <ComboBox x:Name="cboxOutputProp65" HorizontalAlignment="Left" Margin="238,177,0,0"
                              VerticalAlignment="Top" Width="98" Height="32" Background="White" FontSize="18"
                              FontFamily="Arial" />
                    <Label x:Name="lblZebra" Content="Zebra" Margin="0,374,30,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="21.333" Height="35"
                           Foreground="#FF005288" FontWeight="Bold" BorderBrush="Black" BorderThickness="0,0,0,1"
                           HorizontalAlignment="Right" Width="570" />
                    <Label x:Name="lblPuertoZebra" Content="Nombre de impresora Zebra:" HorizontalAlignment="Left"
                           Margin="30,423,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="264" />
                    <Button x:Name="btPrintTest" Content="Test Print" HorizontalAlignment="Left" Margin="195,466,0,0"
                            VerticalAlignment="Top" Width="204" Height="41" IsDefault="True" TabIndex="8"
                            Click="btPrintTest_Click" />
                    <TextBox x:Name="txbZebra" HorizontalAlignment="Left" Margin="294,422,0,0" TextWrapping="Wrap"
                             Text="" VerticalAlignment="Top" Width="256" Height="34" VerticalContentAlignment="Center"
                             HorizontalContentAlignment="Center" />
                    <Label x:Name="lblBaudSea" Content="Baudrate Sealevel:" HorizontalAlignment="Left"
                           Margin="315,24,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="170" />
                    <ComboBox x:Name="cboxBaudSea" HorizontalAlignment="Left" Margin="480,24,0,0"
                              VerticalAlignment="Top" Width="126" Height="32" Background="White" SelectedIndex="4"
                              FontSize="18" VerticalContentAlignment="Center" HorizontalContentAlignment="Center"
                              FontFamily="Arial">
                        <ComboBoxItem Content="9600" />
                        <ComboBoxItem Content="19200" />
                        <ComboBoxItem Content="38400" />
                        <ComboBoxItem Content="57600" />
                        <ComboBoxItem Content="115200" />
                    </ComboBox>
                    <Label Content="Sensor D. master" HorizontalAlignment="Left" Margin="318,268,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="210"
                           Visibility="Visible" />
                    <ComboBox x:Name="cboxInputMaster" HorizontalAlignment="Left" Margin="480,268,0,0"
                              VerticalAlignment="Top" Width="126" Height="32" Background="White" FontSize="18"
                              FontFamily="Arial" />
                    <Label Content="Dispensadora master" HorizontalAlignment="Left" Margin="338,133,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="205" />
                    <ComboBox x:Name="cboxOutputMaster" HorizontalAlignment="Left" Margin="528,133,0,0"
                              VerticalAlignment="Top" Width="78" Height="32" Background="White" FontSize="18"
                              FontFamily="Arial" />
                    <Label Content="Selladora" HorizontalAlignment="Left" Margin="341,177,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="205" />
                    <ComboBox x:Name="cboxOutputSelladora" HorizontalAlignment="Left" Margin="452,177,0,0"
                              VerticalAlignment="Top" Width="154" Height="32" Background="White" FontSize="18"
                              FontFamily="Arial" />
                    <Label Content="Sensor selladora" HorizontalAlignment="Left" Margin="318,309,0,0"
                           VerticalAlignment="Top" FontFamily="Arial" FontSize="18.667" Height="32" Width="210"
                           Visibility="Visible" />
                    <ComboBox x:Name="cboxInputSelladora" HorizontalAlignment="Left" Margin="480,309,0,0"
                              VerticalAlignment="Top" Width="126" Height="32" Background="White" FontSize="18"
                              FontFamily="Arial" />
                </Grid>
            </TabItem>
        </TabControl>

        <Button x:Name="btnCancel" Content="Cancelar" HorizontalAlignment="Right" Margin="0,0,22,18"
                VerticalAlignment="Bottom" Width="158" Height="48" Grid.Column="1" Grid.Row="2" IsCancel="True"
                Click="OnCancelButtonClick" TabIndex="9" />
        <Button x:Name="btnSave" Content="Guardar" HorizontalAlignment="Right" Margin="0,0,202,18"
                VerticalAlignment="Bottom" Width="158" Height="48" Grid.Column="1" Grid.Row="2" IsDefault="True"
                Click="OnSaveButtonClick" TabIndex="8" />
    </Grid>


</Window>